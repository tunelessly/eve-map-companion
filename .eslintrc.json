{
    "env": {
        "browser": true,
        "es2021": true
    },
    "plugins": [
        "neverthrow"
    ],
    "extends": "standard-with-typescript",
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "neverthrow/must-use-result": "error"
    }
}